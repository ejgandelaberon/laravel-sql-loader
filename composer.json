{
    "name": "yajra/laravel-sql-loader",
    "description": "Oracle SQL Loader for Laravel",
    "keywords": [
        "laravel",
        "oracle",
        "oci8",
        "sqlldr"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-oci8": ">=3.0.1",
        "ext-pdo": "*",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "yajra/laravel-oci8": "^10.0|^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9.1",
        "laravel/pint": "^1.14",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^9.1.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "*",
        "rector/rector": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\SQLLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\SQLLoader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\SQLLoader\\SQLLoaderServiceProvider"
            ]
        }
    },
    "scripts": {
        "docker": "docker run -d -p 49160:22 -p 49161:1521 deepdiver/docker-oracle-xe-11g"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
